*.lo

# Precompiled headers
*.gch
*.pch
*.gch

# Shared and static libraries
*.a
*.la
*.lai
*.lib
*.so
*.so.*
*.dylib
*.dll

# Executables
*.exe
*.out
*.app
*.i*86
*.x86_64
*.hex

# Debug files
*.dSYM/
*.su
*.idb
*.pdb

# Generated files
*.mod
*.smod
*.d

# IDE/editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS-generated files
.DS_Store
Thumbs.db

# Logs (Meson test logs, etc.)
meson-log.txt
